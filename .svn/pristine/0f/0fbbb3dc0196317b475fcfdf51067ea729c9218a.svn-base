using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace Terran_Command_Center.DataModel
{
    public class Hat
    {

        public String _Name { get; set; }
        public String _Responsibilities { get; set; }
        public String _EndResults { get; set; }
        public List<Staff> _Staff { get; set; }
        public Guid _ID { get; set; }



        public Hat(String Name, String Responsibilities, String EndResults, List<Staff> Staff, Guid ID)
        {
            this._Name = Name;
            this._Responsibilities = Responsibilities;
            this._EndResults = EndResults;
            this._Staff = Staff;
            this._ID = ID;
        }
    }
}