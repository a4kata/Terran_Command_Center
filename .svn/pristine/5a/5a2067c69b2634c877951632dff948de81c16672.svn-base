using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace Terran_Command_Center.DataModel
{
    public class Department
    {
        public String _Name { get; set; }
        public List<Metric> _Fields { get; set; }
        public String _Description { get; set; }
        public int _Number { get; set; }
        public String _FVP { get; set; }
        public List<Hat> _Hats { get; set; }
        public Guid _ID { get; set; }



        public Department(String Name, List<Metric> Fields, String Description, int Number, String FVP, List<Hat> _Hats, Guid ID)
        {
            this._Name = Name;
            this._Fields = Fields;
            this._Description = Description;
            this._Number = Number;
            this._FVP = FVP;
            this._Hats = _Hats;
            this._ID = ID;
        }
    }
}